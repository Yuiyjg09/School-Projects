package storage;

import java.util.*;

import application.model.*;

public class Storage {
	private static ArrayList<Bil> biler;
	private static ArrayList<Kunde> kunder;
	private static ArrayList<BilKatagori> katagorier;
	private static ArrayList<Udlejning> udlejninger;
	
	
	
	public static ArrayList<Bil> getBiler() {
		return biler;
	}
	
	public static void addBil(Bil bil) {
		Storage.biler.add(bil);
	}
	
	public static ArrayList<Kunde> getKunder() {
		return kunder;
	}
	
	public static void setKunder(ArrayList<Kunde> kunder) {
		Storage.kunder = kunder;
	}
	
	public static ArrayList<BilKatagori> getKatagorier() {
		return katagorier;
	}
	
	public static void setKatagorier(ArrayList<BilKatagori> katagorier) {
		Storage.katagorier = katagorier;
	}
	
	public static ArrayList<Udlejning> getUdlejninger() {
		return udlejninger;
	}
	
	public static void setUdlejninger(ArrayList<Udlejning> udlejninger) {
		Storage.udlejninger = udlejninger;
	}
	
	
	
}
